{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "environments": [
        "environments/*"
      ],
      "assets/*": [
        "assets/*"
      ],
      "store/*": [
        "app/store/*"
      ],
      "shared/*": [
        "app/shared/*"
      ],
      "core/*": [
        "app/core/*"
      ],
      "entry/*": [
        "app/modules/entry/*"
      ],
      "ichat/*": [
        "app/modules/ichat/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": true
  }
}
